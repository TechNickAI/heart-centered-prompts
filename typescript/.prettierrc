{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "semi": true,
  "overrides": [
    {
      "files": ["*.json", "*.jsonc"],
      "options": { "tabWidth": 2 }
    },
    {
      "files": ["*.md"],
      "options": { "tabWidth": 2, "proseWrap": "always" }
    }
  ]
}
